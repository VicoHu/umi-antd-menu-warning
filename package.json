{
  "private": true,
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage",
    "StartSSRServe": "node server.js"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/pro-layout": "^6.5.0",
    "@umijs/plugin-esbuild": "^1.3.1",
    "antd": "^4.16.6",
    "cross-env": "^7.0.3",
    "express": "^4.17.1",
    "react": "17.x",
    "react-dom": "17.x",
    "umi": "^3.5.5",
    "umi-server": "^1.2.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.5.5",
    "koa": "^2.13.1",
    "koa-compress": "^5.0.1",
    "koa-mount": "^4.0.0",
    "koa-static": "^5.0.0",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.0",
    "ts-node": "^10.0.0",
    "typescript": "^4.1.2",
    "yorkie": "^2.0.0"
  }
}
